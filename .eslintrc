{
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "error",
    "new-cap": [
      "error",
      {
        "capIsNew": false
      }
    ]
  }
}
